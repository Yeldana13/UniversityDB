<script>

(function ($) {
    "use strict";
})(jQuery);
    const body = document.querySelector('body');
    const searchBtn = document.querySelector('#search');
    const searchInput = document.querySelector('#search-input');
    const searchBtn1 = document.querySelector('#search1');
    const searchInput1 = document.querySelector('#search-input1');
    const searchSubmit = document.querySelector('.searchSubmit');

    let active = false;
    let active1 = false;
    let firstSearch = false;
    let secondSearch = false;
body.addEventListener('click', (e) => {
<!--  if(e.target.id === 'search' || e.target.id === 'search-input' || e.target.id === 'search-icon') {-->
    if(e.target.id === 'search' || e.target.id === 'search-icon') {
    if(!active) {
      searchBtn.classList.add('active');
      searchInput.classList.add('active');
      active = true;
    }
    else {
      searchBtn.classList.remove('active');
      searchInput.classList.remove('active');
      searchInput.value = '';
      active = false;
    }
  } else if(e.target.id === 'search1' || e.target.id === 'search-icon1') {
    if(!active1) {
      searchBtn1.classList.add('active');
      searchInput1.classList.add('active');
      active1 = true;
    }

    else {
      searchBtn1.classList.remove('active');
      searchInput1.classList.remove('active');
      searchInput1.value = '';
      active1 = false;
    }
  }
});
    function OnSelectionChange (select) {
            firstSearch = true;
        }
    function OnSelectionChange1 (select) {
            secondSearch = true;
            if(firstSearch && secondSearch){
                searchSubmit.click();
            }
        }

    function searchProfs (select) {
        let prof = select.innerText;
        var url = 'http://localhost/profSearch';
        var form = $('<form action="' + url + '" method="post">' +
          '<input type="text" name="value" value="' + prof + '" />' +
          '</form>');
        $('body').append(form);
        form.submit();
    }
</script>
