        body {
          height: 80vh;
          color: #FFF;
          background: rgb(195,123,255);
          background: -moz-linear-gradient(90deg, rgba(195,123,255,1) 0%, rgba(131,180,237,1) 100%);
          background: -webkit-linear-gradient(90deg, rgba(195,123,255,1) 0%, rgba(131,180,237,1) 100%);
          background: linear-gradient(90deg, rgba(195,123,255,1) 0%, rgba(131,180,237,1) 100%);
          filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#c37bff",endColorstr="#83b4ed",GradientType=1);
        }

        #search , #search1{
          height: 80px;
          width: 80px;
          border: solid 5px;
          border-radius: 50px;
          display: flex;
          justify-content: center;
          align-items: center;
          font-size: 35px;
          transition: 0.3s;
        }

        #search-input , #search-input1{
          height: 100%;
          width: 0px;
          font-size: 28px;
          font-weight: 600;
          background: none;
          color: #FFF;
          border: none;
          outline: 0;
          visibility: hidden;
          transition: 0.3s;
        }

        #search.active, #search1.active {
          width: 350px;
        }

        #search-input.active, #search-input1.active {
          width: 260px;
          margin-left: 16px;
          visibility: visible;
          padding-left: 22px;
        }
        #search, #search1 {
            background-image: url("https://icon-library.net/images/white-search-icon-transparent-background/white-search-icon-transparent-background-1.jpg");
            background-repeat-x: no-repeat;
            background-repeat-y: no-repeat;
            background-size: contain;
            height: auto;
        }
        .searchBar {
            margin: auto;
            width:60%;
            justify-content: center;
        }

    </style>
</head>
<body>
<form class="searchBar" style="display:flex" action="profSearch" method="post">
    <div id="search">
        <i class="fas fa-search" id="search-icon"><img></i>
        <input type="text" id="search-input" name="value" autocomplete="off">
    </div>
</form>
<div class="limiter">
    <div class="container-table100">
        <div class="wrap-table100">
            <div class="table100">
                <table>
                    <thead>
                    <tr class="table100-head">
                        <th class="column1">Специальность</th>
                        <th class="column2">Название</th>
                        <th class="column3">Университет</th>
                        <th class="column4">Университет АЙДИ</th>
                        <th class="column5">Город</th>
                        <th class="column6">Улица</th>
                        <th class="column7">Номер Телефона</th>
                        <th class="column8">Сайт</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#prof}}
                        <tr onclick="ProfClicked(this)">
                            <td class="column1">{{speciality}}</td>
                            <td class="column2">{{name}}</td>
                            <td class="column3">{{university}}</td>
                            <td class="column4">{{university_id}}</td>
                            <td class="column5">{{city}}</td>
                            <td class="column6">{{street}}</td>
                            <td class="column7">{{number}}</td>
                            <td class="column8">{{url}}</td>
                        </tr>
                    {{/prof}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

</body>
